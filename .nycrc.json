{
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "spec/**/*.js",
    "coverage/**",
    "node_modules/**",
    "*.config.js",
    "app.js",
    "debug-test.js",
    "test-simple.js"
  ],
  "extension": [
    ".js"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov"
  ],
  "check-coverage": true,
  "lines": 85,
  "functions": 85,
  "branches": 80,
  "statements": 85,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [75, 90],
    "statements": [80, 95]
  },
  "cache": false,
  "all": true,
  "skip-full": false,
  "instrument": true,
  "sourceMap": false,
  "produce-source-map": false
}